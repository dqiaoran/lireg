
export(lireg)
import(Matrix)

## methods used by R itself.
export(print.summary.lireg, summary.lireg, print.lireg)

## register all the methods for generics elsewhere in case namespace is
## loaded but not currently attached.
S3method(print, lireg)
S3method(print, summary.lireg)
S3method(summary, lireg)
